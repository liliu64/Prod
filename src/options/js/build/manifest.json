{
  "manifest_version": 2,

  "name": "Demo React-Chrome extension",
  "description": "This extension shows how to run a React app as a Chrome extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "logo_small.png"
    // "default_popup": "index.html"
  },
  "options_page": "index.html",
  "permissions": [
    "chrome://favicon/",
    "contentSettings",
    "idle",
    "storage",
    "tabs",
    "<all_urls>",
    "idle",
    "storage"
  ]
}