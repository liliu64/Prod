{
  "name": "Prod",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Stay Productive",
  "homepage_url": "http://www.cs.princeton.edu/~jmorone/prod",
  "content_security_policy": "script-src 'self' https://d3js.org; object-src 'self'",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/browser_action/common.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Prod: Browser Productivity Tool",
    "default_popup": "src/browser_action/main_window.html"
  },
  "permissions": [
    "chrome://favicon/",
    "contentSettings",
    "idle",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/content/content.js"
      ]
    }
  ]
}
